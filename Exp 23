#include <stdio.h>
#define MAX 10
#define INF 999

int main(){
    int n,a[MAX][MAX],dist[MAX],visited[MAX]={0},i,j,min,u;
    scanf("%d",&n);

    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&a[i][j]);

    for(i=0;i<n;i++) dist[i]=INF;
    dist[0]=0;

    for(i=0;i<n;i++){
        min=INF;
        for(j=0;j<n;j++)
            if(!visited[j] && dist[j]<min){
                min=dist[j]; u=j;
            }
        visited[u]=1;
        for(j=0;j<n;j++)
            if(a[u][j] && dist[u]+a[u][j]<dist[j])
                dist[j]=dist[u]+a[u][j];
    }

    for(i=0;i<n;i++)
        printf("%d ",dist[i]);
}
