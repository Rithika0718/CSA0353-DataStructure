#include <stdio.h>
#define MAX 10

int parent[MAX];

int find(int i){
    while(parent[i]) i=parent[i];
    return i;
}

void union_set(int i,int j){
    if(i!=j) parent[j]=i;
}

int main(){
    int n,i,j,a[MAX][MAX],min,cost=0,u,v;
    scanf("%d",&n);

    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&a[i][j]);

    for(int e=0;e<n-1;e++){
        min=999;
        for(i=0;i<n;i++)
            for(j=0;j<n;j++)
                if(a[i][j]<min && a[i][j]!=0){
                    min=a[i][j]; u=i; v=j;
                }
        int x=find(u), y=find(v);
        if(x!=y){
            cost+=min;
            union_set(x,y);
        }
        a[u][v]=a[v][u]=999;
    }
    printf("Cost=%d",cost);
}
